input[type=datetime-local],input[type=date] {
  height: 18px;
  line-height: 18px;
}

.ui-icon-link {
  display: inline-block;
  height: 0;
  position: relative;
  overflow: visible;
}

.ui-icon-link:before {
  background: url(../img/icon.png) no-repeat 0 0;
  -webkit-background-size: 150px auto;
  content: "";
  display: block;
  position: absolute;
  left: 0;
}

.ui-icon-close-btn {
  width: 33px;
  height: 44px;
  font-size:24px!important;
  line-height: 44px!important;
  color: rgba(0, 0, 0, 0.2);
  text-align: center;
  position: absolute;
  right: 0;
  top: 0;
}

.ui-icon-link {
  width: 31px;
  height: 31px;
  position: absolute;
  right: 0;
  top: 7px; }

.ui-icon-link:before {
  background-position: 0 -75px;
  width: 15px;
  height: 15px;
  margin: 8px;
  position: static; }

.ui-tooltips-cnt {
  background-color:#fff;
  line-height:44px;
  height:44px;
  padding-left:10px;
  padding-right:30px;
  max-width:100%;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis }

.ui-list > li {
  margin-left: 10px; }

/*.ui-list-img {
  height: 80px;
  margin: 0 10px 0 0; }*/

.ui-list-info {
  padding-right: 10px; }

.ui-list-info > h4 {
  font-size: 14px;
  line-height: 18px; }

.ui-list-info > p {
  font-size: 12px;
  line-height: 18px;
  color: #a3a6aa; }

/* 列表链接 */
.list-link {
  position: absolute;
  left: 0;
  top: 0;
  width:100%;
  height: 100%;
}
.list-link a {
  display: block;
  width: 100%;
  height: 100%;
}

.ui-list-function > li {
  padding: 0;
}

.ui-tag-new:before {
  background: url(../img/icon_tag.png);
  -webkit-background-size: 36px auto; }
.ui-tag-new:before,.ui-tag-new:after {
  display: block;
  position: absolute;
  content: '';
  height: 20px;
  left: 0;
  top: 0;
  z-index: 9; }

.ui-tag-new:before {
  background-position: 0 -30px;
  width: 24px;
  text-align: right;
  line-height: 20px;
  font-size: 12px;
  color: #fff;
  padding-right: 10px;
  padding-left: 2px; }
.ui-tag-new:before {
  content: '官方';
  background-position: 0 -60px; }
.ui-select {
  position: relative;
  margin-right: 6px; }

.ui-select select {
  -webkit-appearance: none;
  border: 0;
  background: none;
  width: 100%;
  padding-right: 14px; }

.ui-select:after {
  position: absolute;
  top: 50%;
  right: 0;
  margin-top: -4px;
  width: 0;
  height: 0;
  border-top: 6px solid;
  border-right: 5px solid transparent;
  border-left: 5px solid transparent;
  color: #a6a6a6;
  content: "";
  pointer-events: none; }

.ui-select-group {
  margin-left: 95px;
  overflow: hidden; }
.ui-select-group .ui-select {
  float: left;}
.ui-select-group .ui-select select {
  padding-left: 0;
  width: auto;
}