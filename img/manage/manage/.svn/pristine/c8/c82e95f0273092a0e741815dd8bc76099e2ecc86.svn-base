package com.hxy.core.community.entity;

import java.io.Serializable;

public class GroupBoard implements Serializable {

	/**
	 * 群组板块
	 */
	private static final long serialVersionUID = 1L;
	
	private long id;  //自增长id
	private String board_name;//版块名称
	private int board_type;//版块类型：
	private String board_logo;//版块LOGO，存储logo的URL地址
	private long oid;//机构ID
	private int memberNum; //群组成员数量
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getBoard_name() {
		return board_name;
	}
	public void setBoard_name(String board_name) {
		this.board_name = board_name;
	}
	public int getBoard_type() {
		return board_type;
	}
	public void setBoard_type(int board_type) {
		this.board_type = board_type;
	}
	public String getBoard_logo() {
		return board_logo;
	}
	public void setBoard_logo(String board_logo) {
		this.board_logo = board_logo;
	}
	public long getOid() {
		return oid;
	}
	public void setOid(long oid) {
		this.oid = oid;
	}
	public int getMemberNum() {
		return memberNum;
	}
	public void setMemberNum(int memberNum) {
		this.memberNum = memberNum;
	}
	
}
