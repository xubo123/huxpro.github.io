package com.hxy.core.appuser.entity;

import java.io.Serializable;
import java.util.Date;

public class AppUser implements Serializable{

	private static final long serialVersionUID = 1L;
	
	/** 自增长id */
	private long id;

	/** 用户id */
	private String user_id;
	
	/** 用户名称 */
	private String user_name;
	
	/** 用户性别[0:femail|1:male] */
	private int user_sex;
	
	/** 个性签名 */
	private String user_signature;
	
	/** 所在城市 */
	private String user_city;
	
	/** 工作单位 */
	private String user_work_unit;
	
	/** 行业 */
	private String user_profession;
	
	/** 兴趣爱号 */
	private String user_avocation;
	
	/** 用户密码 */
	private String user_password;
	
	/** 用户头像 */
	private String user_avatar;
	
	/** 用户类型[0:china|1:overseas] */
	private int user_type = 0;
	
	/** 手机号 */
	private String user_mobile;
	
	/** 校友卡号 */
	private String user_alumni_card_id;
	
	/** 邮箱 */
	private String user_email;
	
	/** 邮箱状态[0: inactive|1: active] */
	private int user_email_validated;
	
	/** 学生表学生id
	 * 学生表学生id,压缩字段，1:n关系，id用逗号‘，’隔离 */
	private String student_id;
	
	private Date created_date;
	
	private Date modified_date;
	
	/** 创建者，手机端注册加入的用户此处为0，后台添加的用户此处为后台用户id */
	private long created_by;
	
	/** 创建者名称，手机端注册加入的用户此处为“手机注册”，后台添加的用户此处为后台用户名称 */
	private String created_user;
	
	/**学习经历   student_id解析出来的对应的所有班级，不同班级号‘，’隔离*/
	private String fullClassName;  
	
	public long getId() {
		return id;
	}
	
	public void setId(long id) {
		this.id = id;
	}
	
	public String getUser_id() {
		return user_id;
	}

	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}

	public String getUser_name() {
		return user_name;
	}

	public void setUser_name(String user_name) {
		this.user_name = user_name;
	}

	public int getUser_sex() {
		return user_sex;
	}

	public void setUser_sex(int user_sex) {
		this.user_sex = user_sex;
	}

	public String getUser_signature() {
		return user_signature;
	}

	public void setUser_signature(String user_signature) {
		this.user_signature = user_signature;
	}

	public String getUser_city() {
		return user_city;
	}

	public void setUser_city(String user_city) {
		this.user_city = user_city;
	}

	public String getUser_work_unit() {
		return user_work_unit;
	}

	public void setUser_work_unit(String user_work_unit) {
		this.user_work_unit = user_work_unit;
	}

	public String getUser_profession() {
		return user_profession;
	}

	public void setUser_profession(String user_profession) {
		this.user_profession = user_profession;
	}

	public String getUser_avocation() {
		return user_avocation;
	}

	public void setUser_avocation(String user_avocation) {
		this.user_avocation = user_avocation;
	}

	public String getUser_password() {
		return user_password;
	}

	public void setUser_password(String user_password) {
		this.user_password = user_password;
	}

	public String getUser_avatar() {
		return user_avatar;
	}

	public void setUser_avatar(String user_avatar) {
		this.user_avatar = user_avatar;
	}

	public int getUser_type() {
		return user_type;
	}

	public void setUser_type(int user_type) {
		this.user_type = user_type;
	}

	public String getUser_mobile() {
		return user_mobile;
	}

	public void setUser_mobile(String user_mobile) {
		this.user_mobile = user_mobile;
	}

	public String getUser_alumni_card_id() {
		return user_alumni_card_id;
	}

	public void setUser_alumni_card_id(String user_alumni_card_id) {
		this.user_alumni_card_id = user_alumni_card_id;
	}

	public String getUser_email() {
		return user_email;
	}

	public void setUser_email(String user_email) {
		this.user_email = user_email;
	}

	public int getUser_email_validated() {
		return user_email_validated;
	}

	public void setUser_email_validated(int user_email_validated) {
		this.user_email_validated = user_email_validated;
	}
	
	public String getStudent_id() {
		return student_id;
	}

	public void setStudent_id(String student_id) {
		this.student_id = student_id;
	}

	public Date getCreated_date() {
		return created_date;
	}
	
	public void setCreated_date(Date created_date) {
		this.created_date = created_date;
	}
	
	public Date getModified_date() {
		return modified_date;
	}
	
	public void setModified_date(Date modified_date) {
		this.modified_date = modified_date;
	}
	
	public long getCreated_by() {
		return created_by;
	}
	
	public void setCreated_by(long created_by) {
		this.created_by = created_by;
	}
	
	public java.lang.String getCreated_user() {
		return created_user;
	}
	
	public void setCreated_user(java.lang.String created_user) {
		this.created_user = created_user;
	}

	public String getFullClassName() {
		return fullClassName;
	}

	public void setFullClassName(String fullClassName) {
		this.fullClassName = fullClassName;
	}
}
