<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hxy.core.region.dao.CityMapper">
	<resultMap type="com.hxy.core.region.entity.City" id="cityResultMap">
		<id column="id" property="id" />
		<result column="cityName" property="cityName" />
		<result column="areaCode" property="areaCode" />
		<result column="provinceId" property="provinceId" />
	</resultMap>
	<select id="selectByProvinceId" parameterType="int" resultMap="cityResultMap">
		select * from city where provinceId=#{provinceId}
	</select>
</mapper>