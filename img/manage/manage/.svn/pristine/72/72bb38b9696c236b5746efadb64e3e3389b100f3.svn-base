package com.hxy.util;

public class ChatGroup {
     String groupname;//群组名称，必须
     String desc;//群主描述，必须
     boolean isPublic;//是否为公开群，必须
     int maxusers;//群主最大成员人数，默认200，最大2000，可选项
     boolean approval;//加入公开群是否需要批准，默认false私有群必须为true,必须
     String owner;//群组的管理员，必须
     String[] members;//群组成员可选
	public String getGroupname() {
		return groupname;
	}
	public void setGroupname(String groupname) {
		this.groupname = groupname;
	}
	public String getDesc() {
		return desc;
	}
	public void setDesc(String desc) {
		this.desc = desc;
	}
	public boolean isPublic() {
		return isPublic;
	}
	public void setPublic(boolean isPublic) {
		this.isPublic = isPublic;
	}
	public int getMaxusers() {
		return maxusers;
	}
	public void setMaxusers(int maxusers) {
		this.maxusers = maxusers;
	}
	public boolean isApproval() {
		return approval;
	}
	public void setApproval(boolean approval) {
		this.approval = approval;
	}
	public String getOwner() {
		return owner;
	}
	public void setOwner(String owner) {
		this.owner = owner;
	}
	public String[] getMembers() {
		return members;
	}
	public void setMembers(String[] members) {
		this.members = members;
	}
}
