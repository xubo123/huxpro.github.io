<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hxy.core.region.dao.CountryMapper">
	<resultMap type="com.hxy.core.region.entity.Country" id="countryResultMap">
		<id column="id" property="id" />
		<result column="countryName" property="countryName" />
		<result column="orderId" property="orderId" />
	</resultMap>
	<select id="selectAll" resultMap="countryResultMap">
		select * from country
	</select>
</mapper>