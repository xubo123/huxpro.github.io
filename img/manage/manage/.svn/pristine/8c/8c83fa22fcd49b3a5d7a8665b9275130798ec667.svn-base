package com.hxy.core.news.entity;

import java.io.Serializable;

/**
 * 新闻栏目
 * @author Xubo
 *
 */
public class NewsChannel implements Serializable{
	/**栏目主键id*/
   private long id;
   /**父栏目id*/
   private long channel_pid;
   /**栏目名称*/
   private String channel_name;
   /**栏目类型*/
   private int channel_type;
   /**栏目链接*/
   private String channel_url;
   /**栏目排序*/
   private String channel_ordernum;
   /**栏目创建者*/
   private long created_by;
public long getId() {
	return id;
}
public void setId(long id) {
	this.id = id;
}
public long getChannel_pid() {
	return channel_pid;
}
public void setChannel_pid(long channel_pid) {
	this.channel_pid = channel_pid;
}
public String getChannel_name() {
	return channel_name;
}
public void setChannel_name(String channel_name) {
	this.channel_name = channel_name;
}
public int getChannel_type() {
	return channel_type;
}
public void setChannel_type(int channel_type) {
	this.channel_type = channel_type;
}
public String getChannel_url() {
	return channel_url;
}
public void setChannel_url(String channel_url) {
	this.channel_url = channel_url;
}
public long getCreated_by() {
	return created_by;
}
public void setCreated_by(long created_by) {
	this.created_by = created_by;
}
public String getChannel_ordernum() {
	return channel_ordernum;
}
public void setChannel_ordernum(String channel_ordernum) {
	this.channel_ordernum = channel_ordernum;
}
}
